require 'json'
require 'csv'
require 'rb-readline'
require 'pry'
require 'rubygems'
require 'rexml/document'
require 'google_custom_search'

GOOGLE_SEARCH_CX = "..."
GOOGLE_SEARCH_PARAMS = {
  :ie => 'utf8',
  :oe => 'utf8'
}

file = File.read('politicians.csv')
data = CSV.parse(file, :headers => true)

puts data.first["First Name"]
results = GoogleCustomSearch.search("#{data.first["First Name"]} #{data.first["Last Name"]} donate")

puts results
